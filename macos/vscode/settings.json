{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/android/**": true,
    "**/ios/**": true,
    "**/build/**": true,
    "**/macos/**": true,
    "**/web/**": true,
  },
  "[javascript]": {
    "editor.defaultFormatter": "numso.prettier-standard-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "breadcrumbs.enabled": true,
  "dart.allowAnalytics": false,
  "dart.analyzeAngularTemplates": false,
  "dart.checkForSdkUpdates": false,
  "dart.debugExternalLibraries": false,
  "dart.debugSdkLibraries": false,
  "dart.previewFlutterOutline": true,
  "dart.previewFlutterStructuredErrors": true,
  "dart.previewFlutterUiGuides": true,
  "dart.promptToRunIfErrors": false,
  "dart.reportAnalyzerErrors": false,
  "dart.showTestCodeLens": false,
  "dart.useKnownChromeOSPorts": false,
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "editor.minimap.enabled": false,
  "editor.suggestSelection": "first",
  "enableTelemetry": false,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "files.autoSave": "afterDelay",
  "git.confirmSync": false,
  "indentRainbow.ignoreErrorLanguages": [
    "*",
    "dart"
  ],
  "macros": {
    "showFileInExplorer": [
      "list.collapseAll",
      "workbench.files.action.showActiveFileInExplorer"
    ]
  },
  "php-project.automaticRestart": true,
  "phpcs.executablePath": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
  "todohighlight.isEnable": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.colorCustomizations": {},
  "workbench.editor.labelFormat": "short",
  "workbench.iconTheme": "material-icon-theme",
  // "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  //   "settings.cycle": [
  //     {
  //         "setting": "workbench.panel.defaultLocation",
  //         "values": [
  //           "right",
  //           "bottom",
  //         ]
  //     }
  // ]
  // ,
  "settings.cycle": [
    {
      "id": "workbenchLocation", // must be unique
      "overrideWorkspaceSettings": true,
      "values": [
        {
          "workbench.panel.defaultLocation": "right",
        },
        {
          "workbench.panel.defaultLocation": "bottom",
        }
      ]
    }
  ],
  "files.autoSaveDelay": 50,
  "dart.lineLength": 200,
  "highlightLine.borderStyle": "outset",
  "explorer.compactFolders": false,
  "search.showLineNumbers": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "zenMode.hideTabs": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "dart.flutterSdkPath": "/Users/user/flutter",
  "editor.linkedEditing": true,
